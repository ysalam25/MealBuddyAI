{
  "api": {
    "mealbuddyapi": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "mealbuddylambda"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "auth": {
    "MealBuddy": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_NUMBERS",
            "REQUIRES_SYMBOLS",
            "REQUIRES_UPPERCASE"
          ],
          "passwordPolicyMinLength": 12
        },
        "signupAttributes": [
          "EMAIL",
          "NAME",
          "GENDER"
        ],
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "function": {
    "mealbuddylambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "parameters": {
    "AMPLIFY_function_mealbuddylambda_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "mealbuddylambda"
        }
      ]
    },
    "AMPLIFY_function_mealbuddylambda_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "mealbuddylambda"
        }
      ]
    }
  }
}